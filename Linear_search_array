#include <stdio.h>

int main(){
    int arr[10] = {1,2,0,3,2,3,0,0,4,5};
    int n = 10;
    
    int num;
    
    printf("Enter the Number you would like to look for: ");
    scanf("%d",&num);
    
    for(int i = 0;i < n;i++){
        if(arr[i] == num){
            printf("Element Found at index: %d",i);
            break;
        }
    }
    
    return 0;
}

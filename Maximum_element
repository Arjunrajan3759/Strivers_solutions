#include <stdio.h>

int main(){
    int n;
    int i;
    int max;
    
    printf("The no. of Elements in the array: ");
    scanf("%d ",&n);
    
    int arr[n];
    
    for(i = 0;i < n;i++){
        printf("Enter the Element[%d]: ",i + 1);
        scanf("%d",&arr[i]);
    }
    
    max = arr[0];
    
    for(i = 0; i < n; i++){
        if(arr[i] > arr[0]){
            max = arr[i];
        }
    }
    
    printf("The Maximum Element in this Array is: %d",max);
    
    return 0;
    
}
